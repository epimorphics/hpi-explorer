@font-face {
  font-family: 'GeoHeadlineRegular';
  src: url(font-path( 'geoheadline-webfont.eot' ));
  src: url(font-path( 'geoheadline-webfont.eot#iefix' )) format('embedded-opentype'),
       url(font-path( 'geoheadline-webfont.woff' )) format('woff'),
       url(font-path( 'geoheadline-webfont.ttf' )) format('truetype'),
       url(font-path( 'geoheadline-webfont.svg#GeoHeadlineRegular' )) format('svg');
  font-weight: normal;
  font-style: normal;
}

.map-button {
  background-image: url( <%= asset_path 'ew-icon.png' %>);
  background-position: center center;
  background-repeat: no-repeat;
  height: 35px;
  width: 35px;
}
