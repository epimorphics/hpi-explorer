.col-md-12
  %section

    #results-header.col-md-12
      = render partial: "hpi/selected_location" if @preferences.location_complete?

    #chart.preview

    %table.table.preview
      %thead
        = indices_table_header( @query_command )
      %tbody
        - @query_command.results.each do |result|
          %tr
            - result.each_with_index do |value, i|
              %td{ class: (i > 0) ? "text-right" : "text-center" }
                = value
